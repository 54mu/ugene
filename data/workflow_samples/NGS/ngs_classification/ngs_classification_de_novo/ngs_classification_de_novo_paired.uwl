    cut-adapt-2.url->quality-trim-2.in-file.url
    get-pe-reads-list.reads-url2->casava-filter-2.in-file.url
    casava-filter-2.url->cut-adapt-2.in-file.url
    quality-trim-2.url->fastqc-after-2.in-file.url

